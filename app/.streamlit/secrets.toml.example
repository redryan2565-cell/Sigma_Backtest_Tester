# Streamlit Secrets Template
# Copy this file to .streamlit/secrets.toml and fill in your values
# DO NOT commit secrets.toml to version control

# Developer Mode (optional)
# Set to "true" to enable Optimization and Leverage Mode tabs
# DEVELOPER_MODE = "false"

# Debug Mode (optional)
# Set to "true" to show detailed error messages
# DEBUG_MODE = "false"

# Cache Settings (optional)
# CACHE_ENABLED = "true"
# CACHE_TTL_HOURS = "24"
# CACHE_DIR = ".cache"

# Alpha Vantage API Key (optional)
# Required only if using Alpha Vantage data feed
# ALPHA_VANTAGE_KEY = "your-api-key-here"

